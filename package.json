{
  "name": "mantine-vaul",
  "private": false,
  "version": "0.1.5",
  "description": "Mantine port of Vaul drawer component optimized for mobile devices.",
  "packageManager": "pnpm@8.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrejNemec/mantine-vaul"
  },
  "keywords": [
    "react",
    "drawer",
    "dialog",
    "modal",
    "mantine",
    "vaul"
  ],
  "author": "Andrej Nemec",
  "license": "MIT",
  "main": "./dist/mantine-vaul.umd.js",
  "module": "./dist/mantine-vaul.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mantine-vaul.es.js",
      "require": "./dist/mantine-vaul.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite --config vite-dev.config.ts",
    "build": "tsc && vite build --config vite-build.config.ts"
  },
  "dependencies": {
    "@use-gesture/vanilla": "^10.3.1",
    "@xstate/react": "^4.1.1",
    "clsx": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-robot": "^0.3.0",
    "robot3": "^0.4.1",
    "xstate": "^5.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@mantine/core": "^7.8.1",
    "@mantine/hooks": "^7.8.1",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@use-gesture/react": "^10.3.1",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "babel-loader": "9.1.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jsdom": "24.0.0",
    "json": "11.0.0",
    "postcss": "8.4.38",
    "postcss-preset-mantine": "^1.14.4",
    "postcss-simple-vars": "^7.0.1",
    "prop-types": "15.8.1",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-plugin-dts": "3.8.3"
  },
  "peerDependencies": {
    "@mantine/core": "^7.8.0",
    "@mantine/hooks": "^7.8.0",
    "@use-gesture/react": "^10.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "files": [
    "dist"
  ]
