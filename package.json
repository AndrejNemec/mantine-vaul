{
  "name": "mantine-vaul",
  "private": false,
  "version": "0.4.9",
  "description": "A drawer component library for mobile devices, leveraging the power of Vaul for seamless integration.",
  "packageManager": "pnpm@8.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrejNemec/mantine-vaul"
  },
  "keywords": [
    "react",
    "drawer",
    "dialog",
    "modal",
    "mantine",
    "vaul"
  ],
  "author": "Andrej Nemec",
  "license": "MIT",
  "main": "./dist/mantine-vaul.umd.js",
  "module": "./dist/mantine-vaul.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mantine-vaul.es.js",
      "require": "./dist/mantine-vaul.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite --config vite-dev.config.ts",
    "build": "tsc && vite build --config vite-build.config.ts"
  },
  "dependencies": {
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@mantine/core": "^7.9.0",
    "@mantine/hooks": "^7.9.0",
    "@types/node": "20.12.8",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "babel-loader": "9.1.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "postcss": "8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-dts": "3.9.0"
  },
  "peerDependencies": {
    "@mantine/core": "^7.9.0",
    "@mantine/hooks": "^7.9.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "files": [
    "dist"
  ]
}
