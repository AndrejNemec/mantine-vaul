{
  "name": "mantine-vaul",
  "private": false,
  "version": "0.5.0",
  "description": "A drawer component library for mobile devices, leveraging the power of Vaul for seamless integration.",
  "packageManager": "pnpm@8.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrejNemec/mantine-vaul"
  },
  "keywords": [
    "react",
    "drawer",
    "dialog",
    "modal",
    "mantine",
    "vaul"
  ],
  "author": "Andrej Nemec",
  "license": "MIT",
  "files": ["dist", "LICENSE"],
	"types": "dist/esm/index.d.ts",
	"main": "dist/cjs/index.cjs",
	"module": "dist/esm/index.js",
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.cts",
				"default": "./dist/cjs/index.cjs"
			}
		},
		"./package.json": "./package.json",
    "./style.css": "./dist/style.css"
	},
  "scripts": {
    "dev": "vite --config vite-dev.config.ts",
    "build": "tsc && vite build --config vite-build.config.ts",
    "build:example": "tsc && vite build --config vite-dev.config.ts"
  },
  "dependencies": {
    "vaul": "^0.9.1",
    "@mantine/code-highlight": "^7.9.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@mantine/core": "^7.9.2",
    "@mantine/hooks": "^7.9.2",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "4.3.0",
    "autoprefixer": "10.4.19",
    "babel-loader": "9.1.3",
    "eslint": "^9.3.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "postcss": "8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-dts": "3.9.1",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "peerDependencies": {
    "@mantine/core": "^7.9.0",
    "@mantine/hooks": "^7.9.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "resolutions": {
    "eslint": "8.56.0"
  }
}
